{
  "name": "webpack-express-typescript-starter",
  "version": "0.0.1",
  "description": "A Node-Express Webpack Starter Kit with Typescript",
  "keywords": [
    "webpack",
    "typescript",
    "express",
    "node"
  ],
  "author": "Davit Iskandaryan <davitiskandaryan12@gmail.com>",
